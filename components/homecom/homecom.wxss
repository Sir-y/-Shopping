/* components/homecom/homecom.wxss */

.good {
}

.good-top {
  font-size: 28rpx;
  color: #333;
  padding: 25rpx 0;
  text-align: center;
}

.good-b {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-evenly;
}

.goof {
  background: white;
  display: flex;
  width: 48%;
  flex-direction: column;
  margin-bottom: 10rpx;
  padding-bottom: 10rpx;
}

.good-box {
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-bottom: 10rpx;
  align-items: center;
}

.good-box image {
  width: 302rpx;
  height: 302rpx;
}

.good-box text {
  width: 100%;
  display: inline-block;
  margin-bottom: 5rpx;
  text-indent: 1em;
}

.good-box .good-title {
  font-size: 28rpx;
  color: #333;
}

.good-box .good-t {
  font-size: 28rpx;
  color: #9c3232;
}

.goodt {
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.goodt text {
  width: 100%;
  color: #333;
  font-size: 33rpx;
  margin-bottom: 5rpx;
  text-indent: 1em;
  height: 35rpx;
  text-align: center;
}

.goodt label {
  width: 70rpx;
  height: 70rpx;
  margin-top: 60rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100% 100%;
}
