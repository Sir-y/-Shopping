/* pages/detail/detail.wxss */
@import "../../wxParse/wxParse.wxss";

page {
  background: #f4f4f4;
}

.det {
  width: 100%;
  height: 750rpx;
}

.det-img {
  width: 100%;
  height: 100%;
}

.det-top {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 20rpx;
}

.det-tbox {
  display: flex;
  justify-content: center;
  align-items: center;
}

.det-tbox .det-tdot {
  width: 8rpx;
  height: 8rpx;
  display: inline-block;
  border-radius: 50%;
  border: 1rpx solid #000;
  margin-right: 5rpx;
}

.det-tbox text {
  font-size: 25rpx;
  color: #666;
}

.det-cen {
  text-align: center;
  height: 306rpx;
  background: #fff;
  margin: 0 auto;
  border-bottom: 1rpx solid #f4f4f4;
}

.det-cen .det-ctext {
  font-size: 41rpx;
  padding: 20rpx;
  color: #333;
}

.det-cen .det-ctext1 {
  margin-bottom: 25rpx;
  font-size: 24rpx;
  color: #999;
}

.det-cen .det-ctext2 {
  margin-top: 10rpx;
  font-size: 35rpx;
  color: #b4282d;
}

.det-shu {
  height: 108rpx;
  background: #fff;
  margin-bottom: 20rpx;
  padding: 0 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.det-shus {
  font-size: 28rpx;
  color: #333;
}

.det-shut {
  width: 52rpx;
  height: 52rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100% 100%;
}

.det-tant {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
}

.det-tanb {
  position: fixed;
  flex-direction: column;
  bottom: -500rpx;
  width: 100%;
  box-sizing: border-box;
  background: white;
  height: 500rpx;
  padding: 30rpx;
}

.det-btop {
  display: flex;
  width: 100%;
  justify-content: space-between;
  margin-bottom: 35rpx;
}

.det-btop1 {
  display: flex;
}

.det-btop1 image {
  height: 177rpx;
  width: 177rpx;
  margin-right: 30rpx;
}

.det-btop1 .det-btleft {
  align-self: flex-end;
  display: flex;
  flex-direction: column;
}

.det-btleft text:nth-of-type(1) {
  font-size: 28rpx;
  color: #b4282d;
}

.det-btleft text:nth-of-type(2) {
  font-size: 28rpx;
  color: #333;
}

.det-btop2 {
  font-size: 30rpx;
  color: #999;
}

.det-bb {
  display: flex;
  flex-direction: column;
}

.det-bb text {
  font-size: 28rpx;
  color: #333;
}

.det-bbs {
  display: flex;
  height: 71rpx;
  width: 322rpx;
  border: 1rpx solid #ccc;
  margin-top: 20rpx;
}

.det-bbs>view {
  width: 90rpx;
  line-height: 71rpx;
  text-align: center;
}

.det-bbs>input {
  flex: 1;
  height: 100%;
  text-align: center;
  border-left: 1rpx solid #ccc;
  border-right: 1rpx solid #ccc;
}

.det-can {
  background: white;
  margin-top: 20rpx;
  padding: 20rpx 30rpx;
  margin-bottom: 20rpx;
}

.det-can .det-cc {
  font-size: 38rpx;
  padding: 20rpx 0;
  color: #333;
}

.det-cbox {
  display: flex;
  background: #f7f7f7;
  padding: 20rpx 0;
  margin: 20rpx;
}

.det-cbox text:nth-of-type(1) {
  width: 134rpx;
  font-size: 25rpx;
  color: #999;
  margin-left: 15rpx;
}

.det-cbox text:nth-of-type(2) {
  flex: 1;
  display: flex;
  font-size: 28rpx;
  color: #333;
}

.det-xtu {
  font-size: 0;
  text-align: center;
}

.det-xtu template {
  margin: 0 auto;
}

.det-wen {
  margin-top: 50rpx;
  margin-bottom: 110rpx;
}

.det-went {
  font-size: 28rpx;
  padding: 35rpx 0;
  background: #fff;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}

.det-went>text {
  margin: 0 25rpx;
}

.det-went1 {
  display: inline-block;
  height: 1rpx;
  width: 100rpx;
  background: #ccc;
}

.det-call {
  padding-bottom: 25rpx;
}

.det-wenb {
  padding: 0rpx 30rpx;
  background: #fff;
}

.det-wenbt {
  display: flex;
  align-items: center;
  padding-bottom: 15rpx;
}

.det-wenbtx {
  height: 8rpx;
  width: 8rpx;
  background: #b4282d;
  border-radius: 50%;
}

.det-wenbt text {
  line-height: 30rpx;
  padding-left: 8rpx;
  display: block;
  font-size: 26rpx;
  color: #303030;
  background: #fff;
}

.det-wenbxt {
  width: 100%;
  line-height: 40rpx;
  padding-left: 16rpx;
  font-size: 26rpx;
  color: #787878;
}

.look-top {
  font-size: 28rpx;
  padding: 35rpx 0;
  background: #fff;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}

.look-top>text {
  margin: 0 25rpx;
}

.look-b {
  display: flex;
  flex-wrap: wrap;
  /* justify-content: space-evenly; */
  width: 100%;
  /* overflow-x: hidden; */
}

.look-bbox {
  display: flex;
  flex-direction: column;
  width: 47%;
  margin-left: 2%;
  background: white;
  margin-bottom: 10rpx;
  padding-bottom: 10rpx;
}

.look-bbox image {
  width: 302rpx;
  height: 302rpx;
  margin: 0 auto;
}

.look-bbox text {
  margin-bottom: 5rpx;
  text-indent: 1em;
}

.look-bbox text:nth-of-type(1) {
  width: 90%;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #333;
}

.look-bbox text:nth-of-type(2) {
  font-size: 28rpx;
  color: #9c3232;
}

.showw {
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  background: #fff;
  z-index: 10;
}

.showw-x, .showw-c {
  height: 100rpx;
  width: 162rpx;
  border: 1rpx solid #f4f4f4;
  display: flex;
  align-items: center;
  justify-content: center;
}

.showw-x>view {
  height: 44rpx;
  width: 44rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100% 100%;
}
.showw-c>view {
position: relative;
}
.showw-c>view image {
  height: 44rpx;
  width: 44rpx;
  
}

.showw-c>view label {
  height: 28rpx;
  width: 28rpx;
  z-index: 10;
  position: absolute;
  top: 0;
  right: 0;
  background: #b4282d;
  text-align: center;
  font-size: 18rpx;
  color: #fff;
  line-height: 28rpx;
  border-radius: 50%;
}

.showw-mm {
  height: 100rpx;
  line-height: 100rpx;
  flex: 1;
  text-align: center;
  color: #333;
  font-size: 28rpx;
  border-top: 1rpx solid #f4f4f4;

}

.showw-j {
  border-top: 1rpx solid #b4282d;
  background: #b4282d;
  line-height: 100rpx;
  height: 100rpx;
  flex: 1;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
}
